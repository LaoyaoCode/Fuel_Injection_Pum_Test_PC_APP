<UserControl x:Class="Fip.MControls.LogMessage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Fip.MControls"
             mc:Ignorable="d" 
             xmlns:icons="http://metro.mahapps.com/winfx/xaml/iconpacks"
             d:DesignHeight="40" d:DesignWidth="187">
    <Grid>
        <TextBlock x:Name="LogMessageTB" Text="XXXXXX" TextTrimming="CharacterEllipsis" VerticalAlignment="Center" Margin="5,0,30,0" Foreground="{DynamicResource NormalTextColor}"/>
        
    </Grid>
</UserControl>
